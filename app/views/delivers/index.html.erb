<p id="notice"><%= notice %></p>

<h1>Delivers</h1>

<table>
  <thead>
    <tr>
      <th>Id module</th>
      <th>File</th>
      <th>Id team</th>
      <th>Grade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @delivers.each do |deliver| %>
      <tr>
        <td><%= deliver.id_module %></td>
        <td><%= deliver.file %></td>
        <td><%= deliver.id_team %></td>
        <td><%= deliver.grade %></td>
        <td><%= link_to 'Show', deliver %></td>
        <td><%= link_to 'Edit', edit_deliver_path(deliver) %></td>
        <td><%= link_to 'Destroy', deliver, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deliver', new_deliver_path %>
